{
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "build": "run-s prod:*",
    "dev:eleventy": "eleventy --serve ",
    "dev:parcel": "parcel watch ./_assets/js/main.js --out-dir ../BGF-build/static",
    "prod:eleventy": "eleventy",
    "prod:parcel": "parcel build ./_assets/js/main.js --out-dir ../BGF-build/static"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "sass": "^1.32.10"
  },
  "dependencies": {
    "bootstrap": "^5.0.0-beta3",
    "jquery": "^3.6.0",
    "poppers.js": "^1.16.1"
  }
}
